{
  "name": "job-test-virgin",
  "version": "0.0.1",
  "description": "Twitter client in Node/TS",
  "main": "dist/server.js",
  "scripts": {
    "test": "ts-node node_modules/tape/bin/tape 'src/**/*.spec.ts' | tap-spec",
    "dev": "nodemon src/server.ts --watch src --exec ts-node --allowJs",
    "debug": "nodemon --inspect src/server.ts --watch src --exec ts-node --allowJs",
    "start": "node dist/server.js",
    "build": "tsc",
    "up": "docker-compose up -d",
    "down": "docker-compose down"
  },
  "keywords": [
    "node",
    "typescript"
  ],
  "author": "Akeel Nazir",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.19.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "4.17.1",
    "express-session": "^1.17.1",
    "http-status-codes": "^1.4.0",
    "mongoose": "5.9.23",
    "morgan": "1.10.0",
    "passport": "0.4.1",
    "passport-twitter": "1.0.4",
    "response-time": "2.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/connect-ensure-login": "^0.1.5",
    "@types/cookie-parser": "1.4.2",
    "@types/core-js": "2.5.3",
    "@types/cors": "2.8.6",
    "@types/dotenv-safe": "^8.1.0",
    "@types/express": "4.17.7",
    "@types/mongodb": "3.5.25",
    "@types/mongoose": "5.7.31",
    "@types/morgan": "1.9.1",
    "@types/node": "14.0.22",
    "@types/passport": "1.0.4",
    "@types/passport-twitter": "1.0.36",
    "@types/response-time": "2.3.4",
    "@types/express-session": "^1.17.0",
    "nodemon": "2.0.4",
    "supertest": "^4.0.2",
    "tap-spec": "5.0.0",
    "tape": "5.0.1",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.9.6"
  }
}
